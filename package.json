{
  "$schema": "https://json.schemastore.org/package",
  "name": "template-typescript",
  "version": "1.0.0",
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@3.6.3",
  "main": "dist/main.js",
  "scripts": {
    "lint": "eslint --fix",
    "test": "jest --passWithNoTests",
    "tsconfig-generate-app": "node tsconfig-generate-app",
    "compile": "npm run lint && rimraf build && node tsconfig-generate-app && tsc -project tsconfig.app.json --rootDir src --outDir build",
    "dist": "npm run compile && rimraf dist && ncc build build/main.js --out dist",
    "build": "npm run dist && npm run test"
  },
  "dependencies": {
    "@actions/core": "1.10.1",
    "@actions/github": "5.1.1",
    "@octokit/plugin-request-log": "4.0.0",
    "@octokit/plugin-retry": "5.0.5",
    "@octokit/plugin-throttling": "5.2.3",
    "console-log-level": "1.4.1"
  },
  "devDependencies": {
    "@tsconfig/node20": "20.1.2",
    "@types/jest": "29.5.5",
    "@types/node": "20.7.2",
    "@types/semver": "^7",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@typescript-eslint/parser": "6.7.3",
    "@vercel/ncc": "0.38.0",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-github": "4.10.1",
    "eslint-plugin-jest": "27.4.2",
    "jest": "29.7.0",
    "jest-circus": "29.7.0",
    "rimraf": "5.0.5",
    "semver": "^7",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2"
  }
}
